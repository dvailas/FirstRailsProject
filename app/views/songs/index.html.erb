<h1>Songs</h1>
<table class="table table-striped">
<thead>
    <tr>
      <th scope="col">Song</th>
      <th scope="col">Artist</th>
      <th scope="col">Link</th>
    </tr>
  </thead>
<tbody>
  <% @songs.each do |song|%>
  <tr>
    <td><%= song.name %></td>
    <td><%= song.artist.name%></td>
    <td><%= link_to 'Show', song_path(song) %></td>
  </tr>
  <% end %>
</tbody>
</table>
